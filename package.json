{
    "name": "allc",
    "version": "0.3.0",
    "description": "A small typescript package for color conversion.",
    "repository": "https://github.com/Trombecher/allc",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "test": "vitest --no-watch",
        "build": "bun build.js"
    },
    "keywords": [
        "Color",
        "Colors",
        "Color Space",
        "Color Model",
        "Conversion",
        "RGB",
        "RGBA",
        "HSL",
        "HSLA",
        "HSV",
        "HSVA",
        "HSI",
        "CIE 1931 XYZ",
        "CIE 1931 xyY",
        "Oklab",
        "Oklch",
        "CIELAB",
        "CIELCH"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "type": "module",
    "author": "Tobias Hillemanns",
    "license": "MIT",
    "devDependencies": {
        "culori": "^4.0.1",
        "typescript": "^5.8.3",
        "vitest": "^3.1.3",
        "@types/bun": "^1.2.12"
    },
    "files": [
        "src",
        "dist",
        "hero.svg",
        "README.md",
        "LICENSE"
    ]
}